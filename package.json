{
  "name": "frontend-web-application",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "deploy": "firebase deploy --token $FIREBASE_TOKEN",
    "build_de_prod": "ng build --prod --i18n-file src/locale/de/messages.de.xlf --i18n-format xlf --i18n-locale de",
    "test_headless": "ng test --watch false --browsers ChromeHeadless",
    "e2e_headless": "ng e2e --protractor-config='e2e/ci.protractor.conf.js'",
    "all_tests": "npm run lint && npm run test_headless && npm run e2e_headless",
    "webdriver": "webdriver-manager update --versions.chrome=78.0.3904.70"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^9.1.1",
    "@angular/common": "^9.1.1",
    "@angular/compiler": "^9.1.1",
    "@angular/core": "^9.1.1",
    "@angular/fire": "^5.4.2",
    "@angular/forms": "^9.1.1",
    "@angular/localize": "^9.1.1",
    "@angular/platform-browser": "^9.1.1",
    "@angular/platform-browser-dynamic": "^9.1.1",
    "@angular/router": "^9.1.1",
    "@ng-bootstrap/ng-bootstrap": "^6.0.3",
    "angular-calendar": "^0.28.5",
    "apexcharts": "^3.18.1",
    "bootstrap": "^4.4.1",
    "chart.js": "^2.9.1",
    "core-js": "^3.3.6",
    "date-fns": "^2.12.0",
    "firebase": "7.14.1",
    "moment": "^2.24.0",
    "ng-apexcharts": "^1.2.3",
    "ng-sidebar": "^9.2.0",
    "ngx-smart-modal": "^7.4.0",
    "ngx-stripe": "^9.0.2",
    "ngx-toastr": "^12.0.1",
    "rxjs": "^6.5.5",
    "ts-money": "^0.4.6",
    "tslib": "^1.10.0",
    "webdriver-manager": "^12.1.7",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.901.1",
    "@angular/cli": "^9.1.2",
    "@angular/compiler-cli": "^9.1.1",
    "@angular/language-service": "^9.1.1",
    "@types/jasmine": "^3.4.6",
    "@types/jasminewd2": "~2.0.8",
    "@types/node": "^13.13.1",
    "codelyzer": "^5.1.2",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.1",
    "karma": "^5.0.2",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "^2.1.1",
    "karma-jasmine": "~3.1.1",
    "karma-jasmine-html-reporter": "~1.5.3",
    "protractor": "^5.4.4",
    "puppeteer": "chrome-81",
    "ts-node": "~8.9.0",
    "tslint": "~6.1.1",
    "typescript": "~3.8.3"
  }
}
