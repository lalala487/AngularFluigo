{
  "name": "frontend-web-application",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "deploy": "firebase deploy --token $FIREBASE_TOKEN",
    "build_de_prod": "ng build --prod --i18n-file src/locale/de/messages.de.xlf --i18n-format xlf --i18n-locale de",
    "test_headless": "ng test --watch false --browsers ChromeHeadless",
    "e2e_headless": "ng e2e --protractor-config='e2e/ci.protractor.conf.js'",
    "all_tests": "npm run lint && npm run test_headless && npm run e2e_headless",
    "webdriver": "webdriver-manager update --versions.chrome=78.0.3904.70"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^8.2.12",
    "@angular/common": "^8.2.12",
    "@angular/compiler": "^8.2.12",
    "@angular/core": "^8.2.12",
    "@angular/fire": "^5.4.2",
    "@angular/forms": "^8.2.12",
    "@angular/platform-browser": "^8.2.12",
    "@angular/platform-browser-dynamic": "^8.2.12",
    "@angular/router": "^8.2.12",
    "@ng-bootstrap/ng-bootstrap": "^5.3.0",
    "angular-calendar": "^0.27.20",
    "bootstrap": "^4.4.1",
    "chart.js": "^2.9.1",
    "core-js": "^3.3.6",
    "date-fns": "^1.30.1",
    "firebase": "^6.6.2",
    "moment": "^2.24.0",
    "ng-sidebarjs": "^4.0.0",
    "ng2-charts": "2.3.0",
    "ngx-smart-modal": "^7.4.0",
    "ngx-stripe": "^7.4.4",
    "ngx-toastr": "^11.2.0",
    "rxjs": "^6.5.4",
    "ts-money": "^0.4.6",
    "tslib": "^1.11.1",
    "webdriver-manager": "^12.1.7",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.803.25",
    "@angular/cli": "^8.3.25",
    "@angular/compiler-cli": "^8.2.14",
    "@angular/language-service": "^8.2.14",
    "@types/jasmine": "^3.4.6",
    "@types/jasminewd2": "~2.0.8",
    "@types/node": "^13.9.0",
    "codelyzer": "^5.2.1",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "^4.4.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "^2.1.1",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "~1.4.2",
    "protractor": "^5.4.3",
    "ts-node": "~8.4.1",
    "tslint": "~5.20.1",
    "typescript": "~3.5.3"
  }
}
